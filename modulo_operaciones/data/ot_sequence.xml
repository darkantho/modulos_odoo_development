<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_ot" model="ir.sequence">
            <field name="name">Sequence OT</field>
            <field name="code">ot_counter</field>
            <field name="prefix">OT</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="suffix">-22</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_RIC" model="ir.sequence">
            <field name="name">RIC sequence</field>
            <field name="code">ric_counter</field>
            <field name="prefix">RIC</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="suffix">-22</field>
            <field name="implementation">standard</field>
            <field name="company_id" eval="False"/>
        </record>

   </data>
</odoo>
